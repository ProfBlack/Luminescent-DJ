	_CHARA_LOOK_RELEASE('R221R0101_CAITLYN')
	_TALK_CLOSE()
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_r221r0101_caitlyn_3', 'Yes, my lady. I believe so.')
	_TALK_CLOSE()
	_CHARA_LOOK_LOCK('R221R0101_DARACH', 'HERO')
	_CHARA_LOOK_LOCK('R221R0101_CAITLYN', 'HERO')
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_r221r0101_caitlyn_4', 'Lady Caitlin is the owner of\nthe Battle Castle, you see.\rYou will find that she can be very\ntaken by superlative battles.\rHowever, for a reason I’m not able to\ndisclose, Lady Caitlin cannot battle\fyou herself.\rConsequently, I partake in battle\nin her ladyship’s place.')
	_FLAG_SET(#2719)
	_CHARA_LOOK_RELEASE('R221R0101_DARACH')
	_CHARA_LOOK_RELEASE('R221R0101_CAITLYN')
	_JUMP('ev_r221r0101_caitlyn_func_45')
ev_r221r0101_caitlyn_func_45:
	_CHARA_LOOK_LOCK('R221R0101_DARACH', 'HERO')
	_CHARA_LOOK_LOCK('R221R0101_CAITLYN', 'HERO')
	_MACRO_TALKMSG('dp_scenario3', '00_msg_r221r0101_caitlyn_5', 'Lady Caitlin would very much\nappreciate viewing a Pokémon battle.\rAs such, I would like to humbly request\nthat you and I partake in one now.')
