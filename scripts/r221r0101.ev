	_TIME_WAIT(30, @SCWK_ANSWER)
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_r221r0101_caitlyn_18', '...\r...')
	_TALK_CLOSE()
	_OBJ_ANIME('R221R0101_CAITLYN', 'anm_r221r0101_caitlyn_face_off')
