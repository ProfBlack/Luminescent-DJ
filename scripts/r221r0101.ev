	_OBJ_ANIME('R221R0101_CAITLYN', 'anm_r221r0101_action_12')
	_OBJ_ANIME_WAIT()
	_OBJ_ANIME('R221R0101_DARACH', 'anm_r221r0101_action_12')
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_r221r0101_caitlyn_20', 'Here is some Tea for you, too.\nIt really is divine.\rThe flavour of this beverage can quench\nthe thirst of anyone in this world.')
