	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_r221r0101_caitlyn_18', '...\r...')
	_TALK_CLOSE()
	_OBJ_ANIME('R221R0101_CAITLYN', 'anm_r221r0101_action_18')
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_r221r0101_caitlyn_19', 'Ah. Thank you, Darach.')
