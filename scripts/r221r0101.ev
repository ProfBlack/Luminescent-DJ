	_OBJ_ANIME('R221R0101_DARACH', 'anm_r221r0101_action_11')
	_OBJ_ANIME_WAIT()
	_MACRO_TALK_KEYWAIT('dp_scenario3', '00_msg_r221r0101_caitlyn_16', 'Fight. Fight. Fight. Fight.\nFight. Fight. Fight. Fight. Fight.')
